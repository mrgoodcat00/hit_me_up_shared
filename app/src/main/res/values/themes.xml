<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="HitMeUp" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="colorAccent">@color/teal_200</item>
        <!-- Customize your theme here. -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowSplashScreenBackground" tools:targetApi="s">@color/white</item>
        <item name="android:windowSplashScreenIconBackgroundColor" tools:targetApi="s">
            @color/white
        </item>
        <item name="android:windowSplashScreenAnimatedIcon" tools:targetApi="s">
            @drawable/ic_splash_temp_logo_vector_animated
        </item>
    </style>
</resources>